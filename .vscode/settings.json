{
  "explorer.autoRevealExclude": {
    "**/node_modules": true
  },
  "files.exclude": {
    "**/.angular": true,
    "**/.github": true,
    "**/.husky": true,
    "**/build": true,
    "**/docs": true,
    "**/node_modules": true,
    "**/public/": true,
    "**/screenshots/": true,
    "**/.editorconfig": true,
    "**/.eslint*": true,
    "**/.gitignore": true,
    "**/.npmrc": true,
    "**/.prettierignore": true,
    "**/.prettierrc": true,
    "**/debug.log": true,
    "**/package-lock.json": true,
    "**/tsconfig*": true,
    "**/tslint.json": true,
    "**/FUNDING.yml": true,
    "**/LICENSE": true,
    "**/.browserslistrc": true,
    "**/.gitkeep": true,
    "**/environments": true,
    "**/src/main.ts": true,
    "**/polyfills.ts": true,
    "**/typings.d.ts": true,
    "**/jest.config.js": true,
    "**/setup-jest.ts": true,
    "**/CNAME": true
  },
  "editor.renderWhitespace": "all",
  "editor.rulers": [80, 140],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "explorer.sortOrderLexicographicOptions": "lower", // Case sensitive file ordering in tree
  "workbench.iconTheme": "material-icon-theme",
  "cSpell.words": [
    "Zuri's",
    "browserconfig",
    "codelyzer",
    "combinators",
    "deniszholob",
    "devkit",
    "factorio",
    "factoriocheatsheet",
    "mstile",
    "ngsw",
    "readonly",
    "rowspan",
    "stylelint",
    "tslib"
  ],
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
