{
  "name": "factorio-cheat-sheet",
  "version": "0.0.0",
  "license": "GPL-3",
  "repository": "github:deniszholob/factorio-cheat-sheet",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 9009",
    "start:prod": "http-server -p 8080 -c-1 public",
    "build": "npm run lint && npm run build:prod",
    "build:sh": "sh ./build-scripts/build.sh",
    "build:dev": "ng build --dev",
    "build:prod": "ng build --prod",
    "lint": "ng lint"
  },
  "private": true,
  "engines": {
    "node": ">= 12.18.2",
    "npm": ">= 6.13.4"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.ts": "tslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@angular/animations": "^9.1.9",
    "@angular/cdk": "^9.2.4",
    "@angular/common": "^9.1.9",
    "@angular/compiler": "^9.1.9",
    "@angular/core": "^9.1.9",
    "@angular/forms": "^9.1.9",
    "@angular/localize": "^9.1.9",
    "@angular/platform-browser": "^9.1.9",
    "@angular/platform-browser-dynamic": "^9.1.9",
    "@angular/router": "^9.1.9",
    "@angular/service-worker": "^9.1.9",
    "@ng-bootstrap/ng-bootstrap": "^6.1.0",
    "bootstrap": "^4.5.0",
    "core-js": "^2.4.1",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.901.7",
    "@angular-devkit/schematics": "^9.1.7",
    "@angular/cli": "^9.1.7",
    "@angular/compiler-cli": "^9.1.9",
    "@angular/language-service": "^9.1.9",
    "@types/node": "^14.0.11",
    "codelyzer": "^5.2.2",
    "http-server": "^0.12.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.9",
    "ts-node": "~7.0.1",
    "tslint": "~6.1.2",
    "typescript": "~3.8.3"
  }
}
