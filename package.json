{
  "name": "factorio-cheat-sheet",
  "version": "0.0.0",
  "license": "GPL-3",
  "repository": "github:deniszholob/factorio-cheat-sheet",
  "scripts": {
    "ng": "ng",
    "start": "ng serve --port 9009",
    "start:prod": "http-server -p 8080 -c-1 public",
    "build": "npm run lint && npm run build:prod",
    "build:sh": "sh ./build-scripts/build.sh",
    "build:dev": "ng build --dev",
    "build:prod": "ng build --prod",
    "lint": "ng lint"
  },
  "private": true,
  "engines": {
    "node": ">= 12.16.3",
    "npm": ">= 6.13.4"
  },
  "lint-staged": {
    "*.js": "eslint",
    "*.ts": "tslint"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/localize": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@angular/service-worker": "^11.2.14",
    "@ng-bootstrap/ng-bootstrap": "^9.1.2",
    "bootstrap": "^4.5.0",
    "core-js": "^2.4.1",
    "node-sass": "^4.14.1",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.14",
    "@angular-devkit/schematics": "^11.2.14",
    "@angular/cli": "^11.2.14",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@types/node": "^14.0.11",
    "codelyzer": "^6.0.0",
    "http-server": "^0.12.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "ts-node": "~7.0.1",
    "tslint": "~6.1.0",
    "typescript": "~4.1.5"
  }
}
